{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    },
    "EnableDebugLogging": false,
    "Path": "<YourLogPath>"
  },
  "Hosting": {
    "Environment": "LocalDev"
  },
  "Security": {
    "ForceHttpsRedirect": true,
    "AllowedCorsUrls": [
      "http://localhost:3000"
    ]
  },
  "Policy": {
    "MaxRetryAttempts": 3,
    "TimeBetweenRetryInSeconds": 1
  },
  "OutputDirectory": "output",
  "ProcessRequestIntervalTimeInSeconds": 120,
  "EnableExportService": true,
  "SecureDataStoreApiServer": "https://localhost:7001/api",
  "AllowedHosts": "*",
  "ConnectionStrings": { "DefaultConnection": "Server=<YourSqlServer>;Database=DocumentFetchingEngine;User Id=sa;Password=<YourPassword>;MultipleActiveResultSets=true" }
}
